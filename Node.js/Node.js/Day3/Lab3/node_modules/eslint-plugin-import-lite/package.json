    "url": "git+https://github.com/9romise/eslint-plugin-import-lite.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-plugin-import-lite/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json",
    "./rule-options": "./dist/dts/rule-options.d.ts"
  },
